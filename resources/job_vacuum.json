{
  "name": "job_vacuum",
  "email_notifications": {
    "no_alert_for_skipped_runs": false
  },
  "webhook_notifications": {
    "on_failure": [
      {
        "id": "a72ac4c7-339c-441c-879e-3e6b5c50db3c"
      }
    ]
  },
  "notification_settings": {
    "no_alert_for_skipped_runs": false,
    "no_alert_for_canceled_runs": false
  },
  "timeout_seconds": 0,
  "schedule": {
    "quartz_cron_expression": "5 0 12 ? * Wed",
    "timezone_id": "America/Sao_Paulo",
    "pause_status": "UNPAUSED"
  },
  "max_concurrent_runs": 1,
  "tasks": [
    {
      "task_key": "cleaned_issuing",
      "run_if": "ALL_SUCCESS",
      "notebook_task": {
        "notebook_path": "/prd/engenharia/vacuum/03_call_vacuum",
        "base_parameters": {
          "database_name": "cleaned_issuing"
        },
        "source": "WORKSPACE"
      },
      "job_cluster_key": "job_vacuum",
      "max_retries": 1,
      "min_retry_interval_millis": 300000,
      "retry_on_timeout": false,
      "timeout_seconds": 0,
      "email_notifications": {},
      "notification_settings": {
        "no_alert_for_skipped_runs": false,
        "no_alert_for_canceled_runs": false,
        "alert_on_last_attempt": false
      }
    }
   
  ],
  "job_clusters": [
    {
      "job_cluster_key": "job_vacuum",
      "new_cluster": {
        "cluster_name": "",
        "spark_version": "12.2.x-scala2.12",
        "spark_conf": {
          "spark.databricks.hive.metastore.glueCatalog.enabled": "true",
          "spark.network.timeout": "600s",
          "spark.rpc.io.backLog": "256"
        },
        "aws_attributes": {
          "first_on_demand": 1,
          "availability": "SPOT_WITH_FALLBACK",
          "zone_id": "auto",
          "instance_profile_arn": "arn:aws:iam::944360682019:instance-profile/DatabricksGlue",
          "spot_bid_price_percent": 100,
          "ebs_volume_count": 0
        },
        "node_type_id": "i3.xlarge",
        "custom_tags": {
          "Squad": "Data"
        },
        "cluster_log_conf": {
          "s3": {
            "destination": "s3://dock-databricks-overwatch/cluster-logs/",
            "region": "us-east-1",
            "enable_encryption": true,
            "canned_acl": "bucket-owner-full-control"
          }
        },
        "spark_env_vars": {
          "PYSPARK_PYTHON": "/databricks/python3/bin/python3"
        },
        "enable_elastic_disk": false,
        "data_security_mode": "SINGLE_USER",
        "runtime_engine": "STANDARD",
        "num_workers": 2
      }
    }
  ],
  "run_as": {
    "user_name": "rafael.garcia@dock.tech"
  }
}